{
    "name": "jest-puppeteer-react",
    "version": "5.1.1",
    "description": "screenshot tests for your react components in chromium using puppeteer & jest",
    "main": "src/index.js",
    "bin": {
        "jestPuppeteerReactDebug": "./bin/debug.js"
    },
    "engines": {
        "node": ">=v8.9"
    },
    "scripts": {
        "pretest": "cd example && yarn install",
        "test": "cd example && yarn test",
        "precommit": "pretty-quick --staged",
        "prettier": "prettier --write \"./**/*.js\"",
        "serve": "cd example && node -e \"require(__dirname + '/../globalSetup.js')({noInfo: false})\""
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/hapag-lloyd/jest-puppeteer-react.git"
    },
    "keywords": [
        "jest",
        "react",
        "components",
        "puppeteer",
        "chrome",
        "screenshot",
        "test",
        "testing",
        "image",
        "diff"
    ],
    "author": "Ansgar Mertens <ansgar@elbstack.com>",
    "contributors": [
        "Stefan Schult <stefan@elbstack.com>",
        "Albino Tonnina <https://github.com/albinotonnina>",
        "Timo Koenig <https://github.com/kaiserkoenig>",
        "Aleksei Androsov <https://github.com/doochik>"
    ],
    "license": "BSD-3-Clause-No-Nuclear-Warranty",
    "bugs": {
        "url": "https://github.com/hapag-lloyd/jest-puppeteer-react/issues"
    },
    "homepage": "https://github.com/hapag-lloyd/jest-puppeteer-react#readme",
    "dependencies": {
        "debug": "^4.1.1",
        "docker-cli-js": "^2.5.2",
        "expect-puppeteer": "^4.1.1",
        "format-util": "^1.0.3",
        "glob": "^7.1.2",
        "jest-each": "^24.8.0",
        "jest-environment-node": "^24.8.0",
        "jest-environment-puppeteer": "4.1.1",
        "jest-image-snapshot": "^2.11.0",
        "jest-puppeteer": "^3.3.1",
        "lodash.merge": "^4.6.1",
        "node-fetch": "^2.5.0",
        "ora": "^3.4.0",
        "pretty-format": "^24.8.0",
        "webpack": "^4.40.2",
        "webpack-dev-server": "^3.8.1"
    },
    "peerDependencies": {
        "@babel/polyfill": "^7.0.0",
        "html-webpack-plugin": "^3.2.0",
        "jest": "^24.5.0",
        "puppeteer": "^1.8.0",
        "react": "^16.2.0",
        "react-dom": "^16.2.0",
        "webpack": "^4.29.1"
    },
    "devDependencies": {
        "@babel/polyfill": "^7.4.4",
        "husky": "^2.2.0",
        "prettier": "^1.17.0",
        "pretty-quick": "^1.10.0",
        "puppeteer": "^1.15.0",
        "react": "^16.8.6",
        "react-dom": "^16.8.6",
        "yalc": "^1.0.0-pre.27"
    },
    "prettier": {
        "tabWidth": 4,
        "singleQuote": true,
        "trailingComma": "es5"
    }
}
